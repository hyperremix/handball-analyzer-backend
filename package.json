{
  "name": "handball-analyzer-backend",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack --webpackPath webpack.config.prod.js",
    "build:dev": "nest build --webpack",
    "build:watch": "nest build -w --webpack",
    "start:sls:local": "sls offline start --stage local",
    "start:sls": "sls offline start",
    "start:local": "concurrently -r -k \"yarn build:watch\" \"yarn start:sls:local\"",
    "start": "concurrently -r -k \"yarn build:watch\" \"yarn start:sls\"",
    "checkTs": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:ci": "jest --ci --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "deploy": "sls deploy",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.226.0",
    "@aws-sdk/lib-dynamodb": "^3.226.0",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@vendia/serverless-express": "^4.10.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1272.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "joi": "^17.7.0",
    "pdf-parse": "^1.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.6.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/aws-lambda": "^8.10.109",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.12",
    "@types/pdf-parse": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "chalk": "5",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "serverless": "^3.25.1",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^12.0.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
